  "authors": [
    {
      "name": "Antony  ",
      "email": "developer@dripdata.net"
    }
  ],
  "require": {
    "ramsey/uuid": "^4.0.1",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "*",
    "laravel/framework": "5.1.*"
  },
  "autoload": {
    "psr-4": {
